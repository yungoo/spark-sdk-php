{
    "name": "sparkproxy/spark-sdk-php",
    "type": "library",
    "description": "SparkProxy Open API SDK for PHP",
    "keywords": [
        "sparkproxy",
        "static house proxy",
        "sdk"
    ],
    "homepage": "http://developer.sparkproxy.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Sparkproxy",
            "email": "support@sparkproxy.com",
            "homepage": "http://www.sparkproxy.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "myclabs/php-enum": "~1.5.2 || ~1.6.6 || ~1.7.7 || ~1.8.4"
    },
    "require-dev": {
        "paragonie/random_compat": ">=2",
        "phpunit/phpunit": "^4.8 || ^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.3.4",
        "squizlabs/php_codesniffer": "^2.3 || ~3.6"
    },
    "autoload": {
        "psr-4": {
            "SparkProxy\\": "src/sparkproxy"
        },
        "files": [
            "src/sparkproxy/functions.php",
            "src/sparkproxy/Http/Middleware/Middleware.php"
        ]
    }
}
